@using AppService.Areas.MultiTabber.Models
@model ErrorDataTableModel

<div class="row">
    <div class="col-xs-12">
        <div class="box">
            <div class="box-header">
                <h3 class="box-title">@Model.TableName</h3>
            <br />
                <small>Date range:<b>@Model.DateRange</b></small>
            </div><!-- /.box-header -->
            <div class="box-body">
                <table class ="table table-bordered table-hover">
                    <thead>
                        <tr>
                            <td>#</td>
                            <td>Error</td>
                            <td>Description</td>
                            <td>Date</td>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (EData m in Model.errData)
                        {
                            <tr>
                                <td>@m.Id</td>
                                <td>@m.err</td>
                                <td>@m.descrip</td>
                                <td>@m.date</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>



