@model IEnumerable<CafeBlue.ProductsTable>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutDashBoard.cshtml";
}

<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        CafeBlue
        <small>Hot Beverage</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="/Dashboard/"><i class="fa fa-dashboard"></i>Home</a></li>
        <li class="active">Hot Beverage</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <!--Product control partial-->
    @Html.Partial("_ProductControlPartial", "hot")
    <!--data table-->
    @Html.Partial("_dataTablePartial", Model)
</section>
<!-- /.content -->

