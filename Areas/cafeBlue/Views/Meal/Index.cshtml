@using AppService.Areas.cafeBlue.Models;
@model IEnumerable<CafeBlue.ProductsTable>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutDashBoard.cshtml";
    List<SelectListItem> items = new List<SelectListItem>();
}

<script type="text/javascript">
    $(document).ready(function (e) {
        $('#modal-29791-new-meal').click(function (e) {
            $('#myModalLabelNewProduct').text('New Meal');
        });
    });
</script>

<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        CafeBlue
        <small>Meals</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="/Dashboard/"><i class="fa fa-dashboard"></i>Home</a></li>
        <li class="active">Meals</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <!--Product control partial-->
    @Html.Partial("_ProductControlPartial", "meal")
    <!--data table-->
    @Html.Partial("_dataTablePartial", Model)
</section>

<!-- /.content -->