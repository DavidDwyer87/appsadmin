@using AppService.Areas.MultiTabber.Models
@model DataTableModel

<div class="row">
    <div class="col-xs-12">
        <div class="box">
            <div class="box-header">
                <h3 class="box-title">@Model.</h3>
            <br />
                <small>Date range:<b>@Model.DateRange</b></small>
            </div><!-- /.box-header -->
            <div class="box-body">
                <table class="table table-bordered table-hover">
                    <thead>
                        <tr>
                            <td>#</td>
                            <td>Email</td>
                            <td>Last Used</td>
                            <td>User Activity</td>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (MData m in Model.MData)
                        {
                            <tr>
                                <td>@m.Id</td>
                                <td>@m.email</td>
                                <td>@m.lastused</td>
                                <td>@m.ActivityCount</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>


